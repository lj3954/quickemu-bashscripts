#!/bin/bash
echo Available VMs: 
cd /home/liam/Documents/VMs && ls
echo
echo Do not input spaces in your selection
echo
read -p 'Select a release (OS + Version): ' rel
read -p 'Select a revision: ' rev
cd "$rel - Revision $rev"
quickemu --vm "${rel// /-}".conf
echo quickemu --vm "${rel// /-}".conf
mkdir abc
